{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      140
    ],
    "no-console": false,
    "arrow-parens": false,
    "jsx-no-lambda": false,
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      false,
      "double"
    ],
    "indent": [
      true,
      "spaces",
      2
    ]
  }
}